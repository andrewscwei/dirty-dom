{
  "name": "dirty-dom",
  "version": "5.5.0",
  "description": "Micro library for handling dirty DOM updates",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && webpack --config config/build.conf.ts",
    "pages": "rimraf .gh-pages && webpack --config demo/config/dev.conf.ts",
    "demo": "npm run build -- --watch & NODE_ENV=development webpack-dev-server --hot --config demo/config/dev.conf.ts --port 8080",
    "test": "echo \"No tests yet\"",
    "lint": "eslint --ext .ts --ext .tsx src config demo",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/dirty-dom.git"
  },
  "keywords": [
    "dirty",
    "dom",
    "updates"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/compression-webpack-plugin": "^9.0.0",
    "@types/debug": "^4.1.7",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^9.0.0",
    "debug": "^4.3.2",
    "eslint": "^8.0.1",
    "html-webpack-plugin": "^5.4.0",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "spase": "^5.1.0"
  }
}
